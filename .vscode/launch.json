{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome remote-WSL",
			"request": "launch",
			"runtimeExecutable": "/mnt/c/Program Files (x86)/Google/Chrome/Application/chrome.exe",
			"sourceMapPathOverrides": {
				"webpack:///./*": "${webRoot}/*"
			},
			"type": "chrome",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		}
	]
}
